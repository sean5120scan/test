@import "../../common/index.wxss";
.idCard-page{
    min-height: 100vh;
    padding-top:80rpx;
    box-sizing: border-box;
}
.main{
    width: 90%;
    margin:0 auto 30rpx auto;
    padding:20rpx 30rpx;
    box-sizing: border-box;
    background:#fff;
    border-radius: 4px;

}
.item{
    display: flex;
    flex-direction:row;
    padding:24rpx;
    border-radius: 12rpx;
    margin:40rpx 0;
    box-sizing: border-box;
  
  }
  .item-icon{
    width:120rpx;
    display: flex;
    padding-left: 24rpx;
    align-items: center;
  }
  .fs{
  font-size: 62rpx;  
  }
  .item-text{
    padding:0 20rpx;
    flex:1;
    box-sizing: border-box
  }
  .info-title{
    font-size:38rpx;
  }
  .info-cont{
    padding-top: 20rpx;
    color:#666
  }
.q-box{
}
.q-title{
    padding:20rpx 0;
}
  .q-item{
      font-size: 36rpx;
      padding:20rpx 0;
      border-bottom:1px solid #ddd;
      color:#666
  }
  .q-item:last-child{
      border-bottom:none;
  }
